le>\n                                {{ language[changeLang].changeLang }}\n                            </v-card-title>\n                            <v-divider></v-divider>\n                            <v-card-text>\n                                <v-radio-group v-model=\"dialogm1\" column>\n                                    <v-radio v-for=\"(item, index) in langs\" :key=\"index\" :label=\"item.title\" :value=\"index\"\n                                        class=\"mr-3\" @click=\"setLang(index)\"></v-radio>\n                                </v-radio-group>\n                            </v-card-text>\n                            <v-divider></v-divider>\n                            <v-card-actions>\n                                <v-btn color=\"blue-darken-1\" variant=\"text\" @click=\"dialog = false\">\n                                    {{ language[changeLang].close }}\n                                </v-btn>\n                            </v-card-actions>\n                        </v-card>\n                    </v-dialog>\n\n                </v-list-item>\n                <v-list-item v-if=\"userLogIn\">\n                    <v-btn variant=\"flat\" @click=\"logout()\">\n                        {{ language[changeLang].logout }}\n                    </v-btn>\n                </v-list-item>\n                <v-list-item v-if=\"!userLogIn\">\n                    <v-btn variant=\"flat\" to=\"/login\">\n                        {{ language[changeLang].login }}\n                    </v-btn>\n                </v-list-item>\n            </v-list>\n        </template>\n    </v-navigation-drawer>\n\n    <v-app-bar>\n        <v-app-bar-nav-icon @click=\"drawer = !drawer\"></v-app-bar-nav-icon>\n        <p class=\"text-h6 ml-2\"> {{ siteNameLang(this.$route.path.substring(1)) }}\n        </p>\n    </v-app-bar>\n</template>\n\n<script>\nexport default {\n    name: 'NavBar',\n    props: {\n        siteName: {\n            type: Number\n        }\n    },\n    data() {\n    