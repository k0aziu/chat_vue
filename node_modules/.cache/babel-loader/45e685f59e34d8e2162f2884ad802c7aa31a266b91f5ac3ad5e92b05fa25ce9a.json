chevron-up' : 'mdi-chevron-down' }}</v-icon>\n            </v-btn>\n        </v-card-actions>\n\n        <v-btn v-if=\"onlyYourContent\">\n            Edytuj etc \n            <!-- TODO -->\n        </v-btn>\n\n        <v-expand-transition>\n            <div v-show=\"show\">\n                <v-divider></v-divider>\n\n                <v-card-text>\n                    {{ cardDesc }}\n                </v-card-text>\n            </div>\n        </v-expand-transition>\n    </v-card>\n</template>\n  \n<script>\nexport default {\n    data: () => ({\n        show: false\n    }),\n    props: {\n        cardName: {\n            type: String,\n            require: true\n        },\n        imageSrc: {\n            type: String,\n            require: true\n        },\n        userAvatar: {\n            type: String,\n            require: true\n        },\n        userName: {\n            type: String,\n            require: true\n        },\n        cardDesc: {\n            type: String,\n            require: true\n        },\n        onlyYourContent: {\n            type: Boolean,\n            require: true\n        }\n    }\n}\n</script>"],"mappings":"AAgEA,eAAe;EACXA,IAAI,EAAEA,CAAA,MAAO;IACTC,IAAI,EAAE;EACV,CAAC,CAAC;EACFC,KAAK,EAAE;IACHC,QAAQ,EAAE;MACNC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACb,CAAC;IACDC,QAAQ,EAAE;MACNH,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACb,CAAC;IACDE,UAAU,EAAE;MACRJ,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACb,CAAC;IACDG,QAAQ,EAAE;MACNL,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACb,CAAC;IACDI,QAAQ,EAAE;MACNN,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACb,CAAC;IACDK,eAAe,EAAE;MACbP,IAAI,EAAEQ,OAAO;MACbN,OAAO,EAAE;IACb;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}